{
  "name": "@1tc/cli",
  "version": "1.0.0",
  "description": "Utility cli for 1tc-platform",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
