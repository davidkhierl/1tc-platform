  },
  "devDependencies": {
    "@1tc/eslint-config": "workspace:*",
    "@1tc/typescript-config": "workspace:*",
    "@1tc/vitest-config": "workspace:*",
    "@types/node": "^24.3.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "typescript": "5.8.3",
    "vitest": "^3.2.4"
  }
}
