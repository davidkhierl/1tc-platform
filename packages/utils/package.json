{
  "name": "@1tc/utils",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./ip": {
      "types": "./src/ip.ts",
      "default": "./dist/ip.js"
    },
    "./netmask": {
      "types": "./src/netmask.ts",
      "default": "./dist/netmask.js"
    },
    "./types/*": {
      "types": "./src/types/*.ts",
      "default": "./dist/types/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "@1tc/eslint-config": "workspace:*",
    "@1tc/typescript-config": "workspace:*",
    "@1tc/vitest-config": "workspace:*",
    "@types/node": "^24.1.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "typescript": "5.8.3",
    "vitest": "^3.2.4"
  }
}
