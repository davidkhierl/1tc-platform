{
  "name": "@repo/supabase-ftp",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./server": {
      "types": "./src/server.ts",
      "default": "./dist/server.js"
    },
    "./helpers/*": {
      "types": "./src/helpers/*.ts",
      "default": "./dist/helpers/*.js"
    },
    "./errors": {
      "types": "./src/errors.ts",
      "default": "./dist/errors.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "tus-js-client": "^4.3.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/utils": "workspace:*",
    "@types/node": "^22.15.3",
    "typescript": "5.8.3"
  }
}
